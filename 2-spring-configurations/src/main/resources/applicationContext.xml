<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="employee" class="com.edureka.spring.springconfigurations.demo1.autowiring.Employee">
        <property name="name" value="Edureka_Employee"/>
    </bean>

    <bean id="companyWithByName" class="com.edureka.spring.springconfigurations.demo1.autowiring.Company" autowire="byName">
    </bean>

    <bean id="companyWithByType" class="com.edureka.spring.springconfigurations.demo1.autowiring.Company" autowire="byType">
    </bean>

    <bean id="companyWithByConstructor" class="com.edureka.spring.springconfigurations.demo1.autowiring.Company" autowire="constructor">
    </bean>
</beans>